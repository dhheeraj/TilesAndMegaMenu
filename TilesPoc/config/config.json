  },
  "externals": {},
  "localizedResources": {
    "HomeTilesWebPartStrings": "lib/webparts/homeTiles/loc/{locale}.js",
    "MegamenuWebPartStrings": "lib/webparts/megamenu/loc/{locale}.js"
  }
}
