{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "home-tiles-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/homeTiles/HomeTilesWebPart.js",
          "manifest": "./src/webparts/homeTiles/HomeTilesWebPart.manifest.json"
        }
      ]
    },
    "megamenu-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/megamenu/MegamenuWebPart.js",
